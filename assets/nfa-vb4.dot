digraph finite_state_machine {
    node [label="", shape=circle];
    
    qs[label=<q<sub>s</sub>>,style="dotted"];
    qp[label=<q'>];
    qf[label=<q<sub>f</sub>>,shape="doublecircle"];    

    qs -> qp[label="0",style="dotted"];
    qp -> qf[label="0"];
    qp -> qp[label="0",style="dotted"];

    rankdir="LR";
}

